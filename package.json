{
  "name": "PrismBrowser",
  "version": "1.0.0",
  "description": "An Electron browser with built-in Clash.Meta",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "update-dashboard": "cmd.exe /c update-dashboard.bat"
  },
  "keywords": [
    "Electron",
    "Browser",
    "Clash"
  ],
  "author": "noNu1L",
  "license": "MIT",
  "devDependencies": {
    "electron": "^34.5.8",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.prismbrowser.app",
    "productName": "PrismBrowser",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules/electron-builder/**/*"
    ],
    "extraResources": [
      {
        "from": "clash-meta",
        "to": "clash-meta",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "electron-store": "^10.0.0"
  }
}
